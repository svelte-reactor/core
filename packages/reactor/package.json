{
  "name": "svelte-reactor",
  "version": "0.3.0",
  "description": "Compatibility wrapper for @svelte-reactor/core. Use @svelte-reactor/core for new projects.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./plugins": {
      "types": "@svelte-reactor/core/plugins",
      "default": "@svelte-reactor/core/plugins"
    },
    "./helpers": {
      "types": "@svelte-reactor/core/helpers",
      "default": "@svelte-reactor/core/helpers"
    },
    "./devtools": {
      "types": "@svelte-reactor/core/devtools",
      "default": "@svelte-reactor/core/devtools"
    },
    "./utils": {
      "types": "@svelte-reactor/core/utils",
      "default": "@svelte-reactor/core/utils"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "svelte",
    "svelte5",
    "state-management",
    "reactive"
  ],
  "author": "Svelte Reactor Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svelte-reactor/core.git",
    "directory": "packages/reactor"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "dependencies": {
    "@svelte-reactor/core": "workspace:^"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
